{
    "version": 2,
    "builds": [
        {
            "src": "server/app.js",
            "use": "@vercel/node"
        },
        {
            "src": "index.html",
            "use": "@vercel/static-build"
        },
        {
            "src": "src/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/server/app.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}