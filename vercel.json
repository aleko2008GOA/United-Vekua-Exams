{
    "version": 2,
    "builds": [
        {
            "src": "server/app.js",
            "use": "@vercel/node"
        },
        {
            "src": "package.json",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "server/app.js"
        },
        {
            "src": "/(.*)",
            "dest": "/dist/index.html"
        }
    ],
    "rewrites": [
        {
            "source": "/:match*",
            "destination": "/index.html"
        }
    ]
}  